package java.com.pasterian.usefulmod.items;

import java.com.pasterian.usefulmod.UsefulStuffMod;

import net.minecraft.item.Item;

public class PurpleShard extends Item {

	public PurpleShard(){
		super();
		this.setCreativeTab(UsefulStuffMod.usefulStuffTabItems);
		this.setMaxStackSize(64);
		this.setTextureName("pasterian" + ":" + "purpleShard");
	}

}