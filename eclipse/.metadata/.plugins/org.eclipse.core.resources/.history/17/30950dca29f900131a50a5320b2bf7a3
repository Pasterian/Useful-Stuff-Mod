package com.pasterian.usefulstuffmod.items;

import net.minecraft.item.Item;

import com.pasterian.usefulstuffmod.UsefulStuffMod;

public class ParGem extends Item {

	public ParGem(){
		super();
		this.setCreativeTab(UsefulStuffMod.usefulStuffTabItems);
		this.setMaxStackSize(64);
		this.setTextureName("pasterian" + ":" + "parmenta_ingot");
	}

}